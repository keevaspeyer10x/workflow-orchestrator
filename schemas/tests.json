{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "test_files": {
      "type": "array",
      "items": {"type": "string"},
      "description": "List of test files created"
    },
    "test_count": {
      "type": "integer",
      "description": "Number of tests written"
    },
    "coverage_areas": {
      "type": "array",
      "items": {"type": "string"},
      "description": "Areas covered by tests"
    }
  }
}
