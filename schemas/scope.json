{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["in_scope", "out_of_scope"],
  "properties": {
    "in_scope": {
      "type": "array",
      "items": {"type": "string"},
      "description": "Features/items that are in scope"
    },
    "out_of_scope": {
      "type": "array",
      "items": {"type": "string"},
      "description": "Features/items that are out of scope"
    },
    "constraints": {
      "type": "array",
      "items": {"type": "string"},
      "description": "Constraints on the implementation"
    }
  }
}
